    <title>converTOO</title>
    <!-- ICON FROM FONTAWESOME -->
    <link rel="icon" href="css/file-excel-solid.svg">
    </head>
    <body class="bg-light"> 
        <div class="container mt-2">
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <% header.forEach(column => { %>
                            <th><%= column %></th>
                        <% }) %>
                    </tr>
                </thead>
                <tbody>
                    <% body.forEach(rows => { %>
                        <tr>
                            <% rows.forEach(row => { %>
                                <td><%= row %> </td>
                            <% }) %>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>
  </body>
</html>